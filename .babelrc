{
  "plugins": [
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "css": "./assets/css"
      },
      "cwd": "babelrc"
    }],
    ["wrap-in-js", {
      "extensions": ["css$", "less$", "scss$"]
    }],
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "lib",
      "style": false
    }],
    "transform-decorators-legacy"
  ],
  "presets": ["env", "stage-0",
    ["next/babel", {
      "styled-jsx": {
        "plugins": [
          "styled-jsx-plugin-sass"
        ]
      }
    }]
  ],
  "env": {
    "development": {
      "plugins": ["inline-dotenv"]
    },
    "production": {
      "plugins": ["transform-inline-environment-variables"]
    }
  }
}
